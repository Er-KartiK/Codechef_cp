import java.util.*;
import java.lang.*;
import java.io.*;

class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		Scanner input=new Scanner(System.in);
		    int a=input.nextInt();
		    int b=input.nextInt();
		    int c=input.nextInt();
		    int x=input.nextInt();
		    if(x==a || x==b || x==c)
		    System.out.println("YES");
		    else
		    System.out.println("NO");
		
	}
}
