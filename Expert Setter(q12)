import java.util.*;
import java.lang.*;
import java.io.*;

class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		Scanner input = new Scanner(System.in);
		int t=input.nextInt();
		for(int i=0;i<t;i++){
		    int x=input.nextInt();
		    int y=input.nextInt();
		    float z=x/2.0f;
		    if(z<=y)
		    System.out.println("YES");
		    else
		    System.out.println("NO");
		}

	}
}
