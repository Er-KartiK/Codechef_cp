import java.util.*;
import java.lang.*;
import java.io.*;

class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		Scanner input = new Scanner(System.in);
		int t= input.nextInt();
		for (int i=0;i<t;i++){
		    int x = input.nextInt();
		    int y= input.nextInt();
		    int z=x*y;
		    System.out.println(z/100);
		}

	}
}
