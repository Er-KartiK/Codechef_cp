           Janmansh starts assigment at x pm find if he can complete the 3 hour assignment at 10pm or not.
import java.util.*;
import java.lang.*;
import java.io.*;

class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		Scanner input = new Scanner(System.in);
		int t=input.nextInt();
		for(int i=0;i<t;i++){
		    int x = input.nextInt();
		    int y=x+3;
		    if(y<=10)
		    System.out.println("YES");
		    else
		    System.out.println("NO");
		}

	}
}
