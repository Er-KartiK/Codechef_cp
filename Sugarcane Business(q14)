import java.util.*;
import java.lang.*;
import java.io.*;

class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		Scanner input = new Scanner(System.in);
		int t= input.nextInt();
		for(int i=0;i<t;i++){
		    int n=input.nextInt();
		    int a=(n*50);
		    int b=a*7/10;
		    n=a-b;
		    System.out.println(n);
		}
	}
}
